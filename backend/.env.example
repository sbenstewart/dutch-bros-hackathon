# Copy this file to .env and fill in values as needed for local development
# AWS credentials (only required if you use AWS services like Transcribe/S3)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-west-2

# Dutch Bros API (optional) - if you want to proxy orders to a real/staging API
DUTCH_BROS_API_BASE_URL=https://pos-api.example.com/v1
DUTCH_BROS_API_KEY=your_api_key_here

# Other optional env vars
# LOG_LEVEL=info
